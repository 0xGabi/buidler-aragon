{
  "name": "@aragon/buidler-aragon",
  "version": "0.0.1-beta.5",
  "description": "Aragon Buidler plugin",
  "main": "dist/index.ts",
  "author": "Aragon Association <legal@aragon.org>",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "aragon",
    "buidler",
    "plugin",
    "ethereum"
  ],
  "files": [
    "contracts",
    "dist",
    "src",
    "buidler.config.js"
  ],
  "scripts": {
    "postinstall": "npm run compile && npm run generate",
    "compile": "buidler compile --force",
    "generate": "typechain --target truffle --outDir typechain 'artifacts/*.json'",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "prettier": "prettier --write '**/*.{ts,js,css,html}'",
    "build-projects": "cd test/projects && npm install && cd ../..",
    "test:mocha": "mocha --exit",
    "test": "npm run build-projects && npm run test:mocha",
    "build": "tsc --build ."
  },
  "dependencies": {
    "@aragon/os": "4.0.1",
    "@nomiclabs/buidler": "^1.0.2",
    "@nomiclabs/buidler-truffle5": "^1.0.2",
    "@nomiclabs/buidler-web3": "^1.0.2",
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@types/web3": "^1.2.2",
    "@types/node": "12.7.5",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "dotenv": "^8.2.0",
    "eth-ens-namehash": "^2.0.8",
    "ethjs-ens": "^2.0.1",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "open": "^7.0.0",
    "static-server": "^2.2.1",
    "truffle-typings": "1.0.4",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typechain": "^1.0.3",
    "typechain-target-truffle": "^1.0.2",
    "typescript": "^3.7.4",
    "web3": "^1.2.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  }
}
